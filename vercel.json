{
  "redirects": [
    {
      "source": "/sap-(:path<[^/]*>)-course-in-(:city<[^/]*>[A-Z][^/]*)",
      "destination": "/sap-$path-course-in-$city",
      "permanent": true
    },
    {
      "source": "/hr-courses-training-institute-in-pune",
      "destination": "/hr-training-course-in-pune",
      "permanent": true
    },
    {
      "source": "/(:path<.*>[A-Z].*)",
      "destination": "/$path",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "dashboard.connectingdotserp.com"
        }
      ],
      "destination": "/dashboard/$1"
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "superadmin.connectingdotserp.com"
        }
      ],
      "destination": "/superadmin/$1"
    }
  ]
}
